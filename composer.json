{
    "name": "itk-dev/kimai-plugin-aarhuskommune",
    "description": "A Kimai plugin for Aarhus Kommune",
    "homepage": "https://github.com/itk-dev/kimai-plugin-AarhusKommuneBundle",
    "type": "kimai-plugin",
    "version": "0.0.0",
    "keywords": [
        "kimai",
        "kimai-plugin"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mikkel Ricky",
            "homepage": "https://github.com/rimi-itk"
        }
    ],
    "extra": {
        "kimai": {
            "require": 21800,
            "name": "Aarhus kommune"
        }
    },
    "autoload": {
        "psr-4": {
            "KimaiPlugin\\AarhusKommuneBundle\\": ""
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": false,
            "symfony/runtime": false
        },
        "platform": {
            "php": "8.1.3"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "scripts": {
        "codestyle": "vendor/bin/php-cs-fixer fix --dry-run --verbose --show-progress=none",
        "codestyle-fix": "vendor/bin/php-cs-fixer fix",
        "codestyle-check": "vendor/bin/php-cs-fixer fix --dry-run --verbose --using-cache=no --show-progress=none --format=checkstyle",
        "phpstan": "vendor/bin/phpstan analyse . --configuration=phpstan.neon --level=8",
        "linting": [
            "composer validate --strict --no-check-version",
            "@codestyle-check",
            "@phpstan"
        ]
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "kimai/kimai": "dev-main",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-doctrine": "^1.0",
        "phpstan/phpstan-symfony": "^1.0",
        "symfony/console": "^6.0",
        "symfony/event-dispatcher": "^6.0"
    }
}
